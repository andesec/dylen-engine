{
  "components": {
    "schemas": {
      "GenerateLessonRequest-Input": {
        "additionalProperties": false,
        "description": "Request payload for lesson generation.",
        "properties": {
          "config": {
            "$ref": "#/components/schemas/GenerationConfig",
            "description": "Configurable generation parameters."
          },
          "constraints": {
            "anyOf": [
              {
                "$ref": "#/components/schemas/GenerationConstraints"
              },
              {
                "type": "null"
              }
            ],
            "description": "Domain-specific generation constraints."
          },
          "idempotency_key": {
            "anyOf": [
              {
                "type": "string"
              },
              {
                "type": "null"
              }
            ],
            "description": "Idempotency key to prevent duplicate lesson generation.",
            "title": "Idempotency Key"
          },
          "prompt": {
            "description": "User-supplied guidance (max 250 words).",
            "minLength": 1,
            "title": "Prompt",
            "type": "string"
          },
          "schema_version": {
            "anyOf": [
              {
                "type": "string"
              },
              {
                "type": "null"
              }
            ],
            "description": "Optional schema version to pin the lesson output to.",
            "title": "Schema Version"
          },
          "topic": {
            "description": "Topic to generate a lesson for.",
            "minLength": 1,
            "title": "Topic",
            "type": "string"
          }
        },
        "required": [
          "topic",
          "prompt"
        ],
        "title": "GenerateLessonRequest",
        "type": "object"
      },
      "GenerateLessonRequest-Output": {
        "additionalProperties": false,
        "description": "Request payload for lesson generation.",
        "properties": {
          "config": {
            "$ref": "#/components/schemas/GenerationConfig",
            "description": "Configurable generation parameters."
          },
          "constraints": {
            "anyOf": [
              {
                "$ref": "#/components/schemas/GenerationConstraints"
              },
              {
                "type": "null"
              }
            ],
            "description": "Domain-specific generation constraints."
          },
          "idempotency_key": {
            "anyOf": [
              {
                "type": "string"
              },
              {
                "type": "null"
              }
            ],
            "description": "Idempotency key to prevent duplicate lesson generation.",
            "title": "Idempotency Key"
          },
          "prompt": {
            "description": "User-supplied guidance (max 250 words).",
            "minLength": 1,
            "title": "Prompt",
            "type": "string"
          },
          "schema_version": {
            "anyOf": [
              {
                "type": "string"
              },
              {
                "type": "null"
              }
            ],
            "description": "Optional schema version to pin the lesson output to.",
            "title": "Schema Version"
          },
          "topic": {
            "description": "Topic to generate a lesson for.",
            "minLength": 1,
            "title": "Topic",
            "type": "string"
          }
        },
        "required": [
          "topic",
          "prompt"
        ],
        "title": "GenerateLessonRequest",
        "type": "object"
      },
      "GenerateLessonResponse": {
        "description": "Response payload for lesson generation.",
        "properties": {
          "lesson_id": {
            "title": "Lesson Id",
            "type": "string"
          },
          "lesson_json": {
            "additionalProperties": true,
            "title": "Lesson Json",
            "type": "object"
          },
          "logs": {
            "items": {
              "type": "string"
            },
            "title": "Logs",
            "type": "array"
          },
          "meta": {
            "$ref": "#/components/schemas/LessonMeta"
          }
        },
        "required": [
          "lesson_id",
          "lesson_json",
          "meta",
          "logs"
        ],
        "title": "GenerateLessonResponse",
        "type": "object"
      },
      "GenerationConfig": {
        "additionalProperties": false,
        "description": "Tunable configuration for lesson generation.",
        "properties": {
          "language": {
            "default": "en",
            "description": "Preferred language for the resulting lesson content.",
            "maxLength": 8,
            "minLength": 2,
            "title": "Language",
            "type": "string"
          },
          "max_output_tokens": {
            "default": 4096,
            "description": "Upper bound on tokens produced during structured generation.",
            "maximum": 65536.0,
            "minimum": 256.0,
            "title": "Max Output Tokens",
            "type": "integer"
          },
          "model": {
            "$ref": "#/components/schemas/GenerationModel",
            "default": "openai/gpt-4o-mini",
            "description": "Model used for lesson structuring."
          },
          "structured_output": {
            "default": true,
            "description": "If false, fall back to raw JSON generation instead of structured mode.",
            "title": "Structured Output",
            "type": "boolean"
          },
          "temperature": {
            "default": 0.4,
            "description": "Sampling temperature for the generation pipeline.",
            "maximum": 2.0,
            "minimum": 0.0,
            "title": "Temperature",
            "type": "number"
          },
          "validation_level": {
            "$ref": "#/components/schemas/ValidationLevel",
            "default": "strict",
            "description": "Level of validation applied to the generated lesson."
          }
        },
        "title": "GenerationConfig",
        "type": "object"
      },
      "GenerationConstraints": {
        "description": "Specific constraints for lesson content generation.",
        "properties": {
          "learnerLevel": {
            "anyOf": [
              {
                "type": "string"
              },
              {
                "type": "null"
              }
            ],
            "title": "Learnerlevel"
          },
          "length": {
            "anyOf": [
              {
                "enum": [
                  "Highlights",
                  "Detailed",
                  "Training"
                ],
                "type": "string"
              },
              {
                "type": "null"
              }
            ],
            "title": "Length"
          },
          "primaryLanguage": {
            "anyOf": [
              {
                "type": "string"
              },
              {
                "type": "null"
              }
            ],
            "title": "Primarylanguage"
          },
          "sections": {
            "anyOf": [
              {
                "maximum": 10.0,
                "minimum": 1.0,
                "type": "integer"
              },
              {
                "type": "null"
              }
            ],
            "title": "Sections"
          }
        },
        "title": "GenerationConstraints",
        "type": "object"
      },
      "GenerationModel": {
        "description": "Supported model identifiers exposed to clients.",
        "enum": [
          "gemini-2.5-pro",
          "gemini-2.5-flash",
          "gemini-2.0-flash",
          "gemini-2.0-flash-exp",
          "gemini-pro-latest",
          "gemini-flash-latest",
          "openai/gpt-4o-mini",
          "openai/gpt-4o",
          "anthropic/claude-3.5-sonnet",
          "google/gemini-2.0-flash-exp:free"
        ],
        "title": "GenerationModel",
        "type": "string"
      },
      "HTTPValidationError": {
        "properties": {
          "detail": {
            "items": {
              "$ref": "#/components/schemas/ValidationError"
            },
            "title": "Detail",
            "type": "array"
          }
        },
        "title": "HTTPValidationError",
        "type": "object"
      },
      "JobCreateResponse": {
        "description": "Response payload for job creation.",
        "properties": {
          "jobId": {
            "title": "Jobid",
            "type": "string"
          }
        },
        "required": [
          "jobId"
        ],
        "title": "JobCreateResponse",
        "type": "object"
      },
      "JobStatusResponse": {
        "description": "Status payload for an asynchronous lesson generation job.",
        "properties": {
          "completedAt": {
            "anyOf": [
              {
                "type": "string"
              },
              {
                "type": "null"
              }
            ],
            "title": "Completedat"
          },
          "cost": {
            "anyOf": [
              {
                "additionalProperties": true,
                "type": "object"
              },
              {
                "type": "null"
              }
            ],
            "title": "Cost"
          },
          "createdAt": {
            "title": "Createdat",
            "type": "string"
          },
          "jobId": {
            "title": "Jobid",
            "type": "string"
          },
          "logs": {
            "items": {
              "type": "string"
            },
            "title": "Logs",
            "type": "array"
          },
          "phase": {
            "anyOf": [
              {
                "type": "string"
              },
              {
                "type": "null"
              }
            ],
            "title": "Phase"
          },
          "progress": {
            "anyOf": [
              {
                "maximum": 1.0,
                "minimum": 0.0,
                "type": "number"
              },
              {
                "type": "null"
              }
            ],
            "description": "Normalized progress indicator when available.",
            "title": "Progress"
          },
          "request": {
            "$ref": "#/components/schemas/GenerateLessonRequest-Output"
          },
          "result": {
            "anyOf": [
              {
                "additionalProperties": true,
                "type": "object"
              },
              {
                "type": "null"
              }
            ],
            "title": "Result"
          },
          "status": {
            "enum": [
              "queued",
              "running",
              "done",
              "error",
              "canceled"
            ],
            "title": "Status",
            "type": "string"
          },
          "subphase": {
            "anyOf": [
              {
                "type": "string"
              },
              {
                "type": "null"
              }
            ],
            "title": "Subphase"
          },
          "updatedAt": {
            "title": "Updatedat",
            "type": "string"
          },
          "validation": {
            "anyOf": [
              {
                "$ref": "#/components/schemas/ValidationResponse"
              },
              {
                "type": "null"
              }
            ]
          }
        },
        "required": [
          "jobId",
          "status",
          "request",
          "createdAt",
          "updatedAt"
        ],
        "title": "JobStatusResponse",
        "type": "object"
      },
      "LessonMeta": {
        "description": "Metadata about the lesson generation process.",
        "properties": {
          "latency_ms": {
            "title": "Latency Ms",
            "type": "integer"
          },
          "model_a": {
            "title": "Model A",
            "type": "string"
          },
          "model_b": {
            "title": "Model B",
            "type": "string"
          },
          "provider_a": {
            "title": "Provider A",
            "type": "string"
          },
          "provider_b": {
            "title": "Provider B",
            "type": "string"
          }
        },
        "required": [
          "provider_a",
          "model_a",
          "provider_b",
          "model_b",
          "latency_ms"
        ],
        "title": "LessonMeta",
        "type": "object"
      },
      "LessonRecordResponse": {
        "description": "Response payload for lesson retrieval.",
        "properties": {
          "created_at": {
            "title": "Created At",
            "type": "string"
          },
          "lesson_id": {
            "title": "Lesson Id",
            "type": "string"
          },
          "lesson_json": {
            "additionalProperties": true,
            "title": "Lesson Json",
            "type": "object"
          },
          "meta": {
            "$ref": "#/components/schemas/LessonMeta"
          },
          "prompt_version": {
            "title": "Prompt Version",
            "type": "string"
          },
          "schema_version": {
            "title": "Schema Version",
            "type": "string"
          },
          "title": {
            "title": "Title",
            "type": "string"
          },
          "topic": {
            "title": "Topic",
            "type": "string"
          }
        },
        "required": [
          "lesson_id",
          "topic",
          "title",
          "created_at",
          "schema_version",
          "prompt_version",
          "lesson_json",
          "meta"
        ],
        "title": "LessonRecordResponse",
        "type": "object"
      },
      "ValidationError": {
        "properties": {
          "loc": {
            "items": {
              "anyOf": [
                {
                  "type": "string"
                },
                {
                  "type": "integer"
                }
              ]
            },
            "title": "Location",
            "type": "array"
          },
          "msg": {
            "title": "Message",
            "type": "string"
          },
          "type": {
            "title": "Error Type",
            "type": "string"
          }
        },
        "required": [
          "loc",
          "msg",
          "type"
        ],
        "title": "ValidationError",
        "type": "object"
      },
      "ValidationLevel": {
        "description": "Supported validation strictness levels.",
        "enum": [
          "basic",
          "strict"
        ],
        "title": "ValidationLevel",
        "type": "string"
      },
      "ValidationResponse": {
        "description": "Response model for lesson validation results.",
        "properties": {
          "errors": {
            "items": {
              "type": "string"
            },
            "title": "Errors",
            "type": "array"
          },
          "ok": {
            "title": "Ok",
            "type": "boolean"
          }
        },
        "required": [
          "ok",
          "errors"
        ],
        "title": "ValidationResponse",
        "type": "object"
      },
      "WritingCheckRequest": {
        "additionalProperties": false,
        "description": "Request payload for response evaluation.",
        "properties": {
          "criteria": {
            "additionalProperties": true,
            "description": "The evaluation criteria from the lesson.",
            "title": "Criteria",
            "type": "object"
          },
          "text": {
            "description": "The user-written response to check (max 300 words).",
            "minLength": 1,
            "title": "Text",
            "type": "string"
          }
        },
        "required": [
          "text",
          "criteria"
        ],
        "title": "WritingCheckRequest",
        "type": "object"
      }
    }
  },
  "info": {
    "title": "FastAPI",
    "version": "0.1.0"
  },
  "openapi": "3.1.0",
  "paths": {
    "/health": {
      "get": {
        "operationId": "health_health_get",
        "responses": {
          "200": {
            "content": {
              "application/json": {
                "schema": {
                  "additionalProperties": {
                    "type": "string"
                  },
                  "title": "Response Health Health Get",
                  "type": "object"
                }
              }
            },
            "description": "Successful Response"
          }
        },
        "summary": "Health"
      }
    },
    "/v1/jobs": {
      "post": {
        "description": "Create a background lesson generation job.",
        "operationId": "create_job_v1_jobs_post",
        "parameters": [
          {
            "in": "header",
            "name": "X-DGS-Dev-Key",
            "required": true,
            "schema": {
              "title": "X-Dgs-Dev-Key",
              "type": "string"
            }
          }
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/GenerateLessonRequest-Input"
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/JobCreateResponse"
                }
              }
            },
            "description": "Successful Response"
          },
          "422": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/HTTPValidationError"
                }
              }
            },
            "description": "Validation Error"
          }
        },
        "summary": "Create Job"
      }
    },
    "/v1/jobs/{job_id}": {
      "get": {
        "description": "Fetch the status and result of a background job.",
        "operationId": "get_job_status_v1_jobs__job_id__get",
        "parameters": [
          {
            "in": "path",
            "name": "job_id",
            "required": true,
            "schema": {
              "title": "Job Id",
              "type": "string"
            }
          },
          {
            "in": "header",
            "name": "X-DGS-Dev-Key",
            "required": true,
            "schema": {
              "title": "X-Dgs-Dev-Key",
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/JobStatusResponse"
                }
              }
            },
            "description": "Successful Response"
          },
          "422": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/HTTPValidationError"
                }
              }
            },
            "description": "Validation Error"
          }
        },
        "summary": "Get Job Status"
      }
    },
    "/v1/jobs/{job_id}/cancel": {
      "post": {
        "description": "Request cancellation of a running background job.",
        "operationId": "cancel_job_v1_jobs__job_id__cancel_post",
        "parameters": [
          {
            "in": "path",
            "name": "job_id",
            "required": true,
            "schema": {
              "title": "Job Id",
              "type": "string"
            }
          },
          {
            "in": "header",
            "name": "X-DGS-Dev-Key",
            "required": true,
            "schema": {
              "title": "X-Dgs-Dev-Key",
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/JobStatusResponse"
                }
              }
            },
            "description": "Successful Response"
          },
          "422": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/HTTPValidationError"
                }
              }
            },
            "description": "Validation Error"
          }
        },
        "summary": "Cancel Job"
      }
    },
    "/v1/lessons/generate": {
      "post": {
        "description": "Generate a lesson from a topic using the two-step pipeline.",
        "operationId": "generate_lesson_v1_lessons_generate_post",
        "parameters": [
          {
            "in": "header",
            "name": "X-DGS-Dev-Key",
            "required": true,
            "schema": {
              "title": "X-Dgs-Dev-Key",
              "type": "string"
            }
          }
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/GenerateLessonRequest-Input"
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/GenerateLessonResponse"
                }
              }
            },
            "description": "Successful Response"
          },
          "422": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/HTTPValidationError"
                }
              }
            },
            "description": "Validation Error"
          }
        },
        "summary": "Generate Lesson"
      }
    },
    "/v1/lessons/jobs": {
      "post": {
        "description": "Alias route for creating a background lesson generation job.",
        "operationId": "create_lesson_job_v1_lessons_jobs_post",
        "parameters": [
          {
            "in": "header",
            "name": "X-DGS-Dev-Key",
            "required": true,
            "schema": {
              "title": "X-Dgs-Dev-Key",
              "type": "string"
            }
          }
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/GenerateLessonRequest-Input"
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/JobCreateResponse"
                }
              }
            },
            "description": "Successful Response"
          },
          "422": {
            "content": {
              "application/json": {
