    "/v1/writing/check": {
      "post": {
        "description": "Create a background job to check a writing task response.",
        "operationId": "create_writing_check_v1_writing_check_post",
        "parameters": [
          {
            "in": "header",
            "name": "X-DGS-Dev-Key",
            "required": true,
            "schema": {
              "title": "X-Dgs-Dev-Key",
              "type": "string"
            }
          }
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/WritingCheckRequest"
              }
            }
          },
          "required": true
        },
        "responses": {
          "202": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/JobCreateResponse"
                }
              }
            },
            "description": "Successful Response"
          },
          "422": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/HTTPValidationError"
                }
              }
            },
            "description": "Validation Error"
          }
        },
        "summary": "Create Writing Check"
      }
