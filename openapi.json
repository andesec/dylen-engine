{
  "components": {
    "schemas": {
      "GenerateLessonRequest": {
        "additionalProperties": false,
        "description": "Request payload for lesson generation.",
        "properties": {
          "blueprint": {
            "anyOf": [
              {
                "enum": [
                  "skillbuilding",
                  "knowledgeunderstanding",
                  "communicationskills",
                  "planningandproductivity",
                  "movementandfitness",
                  "growthmindset",
                  "criticalthinking",
                  "creativeskills",
                  "webdevandcoding",
                  "languagepractice"
                ],
                "type": "string"
              },
              {
                "type": "null"
              }
            ],
            "description": "Optional blueprint or learning outcome guidance for lesson planning.",
            "title": "Blueprint"
          },
          "depth": {
            "default": "highlights",
            "description": "Requested lesson depth (Highlights=2, Detailed=6, Training=10).",
            "enum": [
              "highlights",
              "detailed",
              "training"
            ],
            "title": "Depth",
            "type": "string"
          },
          "details": {
            "anyOf": [
              {
                "minLength": 1,
                "type": "string"
              },
              {
                "type": "null"
              }
            ],
            "description": "Optional user-supplied details (max 250 words).",
            "examples": [
              "Focus on lists and loops"
            ],
            "title": "Details"
          },
          "idempotency_key": {
            "anyOf": [
              {
                "type": "string"
              },
              {
                "type": "null"
              }
            ],
            "description": "Idempotency key to prevent duplicate lesson generation.",
            "title": "Idempotency Key"
          },
          "learner_level": {
            "anyOf": [
              {
                "minLength": 1,
                "type": "string"
              },
              {
                "type": "null"
              }
            ],
            "description": "Optional learner level hint used for prompt guidance.",
            "title": "Learner Level"
          },
          "models": {
            "anyOf": [
              {
                "$ref": "#/components/schemas/ModelsConfig"
              },
              {
                "type": "null"
              }
            ],
            "description": "Optional per-agent model selection overrides.",
            "examples": [
              {
                "gatherer_model": "xiaomi/mimo-v2-flash:free",
                "planner_model": "openai/gpt-oss-120b:free",
                "repairer_model": "google/gemma-3-27b-it:free",
                "structurer_model": "openai/gpt-oss-20b:free"
              }
            ]
          },
          "primary_language": {
            "default": "English",
            "description": "Primary language for lesson output.",
            "enum": [
              "English",
              "German",
              "Urdu"
            ],
            "title": "Primary Language",
            "type": "string"
          },
          "schema_version": {
            "anyOf": [
              {
                "type": "string"
              },
              {
                "type": "null"
              }
            ],
            "description": "Optional schema version to pin the lesson output to.",
            "title": "Schema Version"
          },
          "teaching_style": {
            "anyOf": [
              {
                "items": {
                  "enum": [
                    "conceptual",
                    "theoretical",
                    "practical"
                  ],
                  "type": "string"
                },
                "type": "array"
              },
              {
                "type": "null"
              }
            ],
            "description": "Optional teaching style or pedagogy guidance for lesson planning.",
            "title": "Teaching Style"
          },
          "topic": {
            "description": "Topic to generate a lesson for.",
            "examples": [
              "Introduction to Python"
            ],
            "minLength": 1,
            "title": "Topic",
            "type": "string"
          },
          "widgets": {
            "anyOf": [
              {
                "items": {
                  "type": "string"
                },
                "maxItems": 8,
                "minItems": 3,
                "type": "array"
              },
              {
                "type": "null"
              }
            ],
            "description": "Optional list of allowed widgets (overrides defaults).",
            "title": "Widgets"
          }
        },
        "required": [
          "topic"
        ],
        "title": "GenerateLessonRequest",
        "type": "object"
      },
      "GenerateLessonResponse": {
        "description": "Response payload for lesson generation.",
        "properties": {
          "lesson_id": {
            "title": "Lesson Id",
            "type": "string"
          },
          "lesson_json": {
            "additionalProperties": true,
            "title": "Lesson Json",
            "type": "object"
          },
          "logs": {
            "items": {
              "type": "string"
            },
            "title": "Logs",
            "type": "array"
          },
          "meta": {
            "$ref": "#/components/schemas/LessonMeta"
          }
        },
        "required": [
          "lesson_id",
          "lesson_json",
          "meta",
          "logs"
        ],
        "title": "GenerateLessonResponse",
        "type": "object"
      },
      "HTTPValidationError": {
        "properties": {
          "detail": {
            "items": {
              "$ref": "#/components/schemas/ValidationError"
            },
            "title": "Detail",
            "type": "array"
          }
        },
        "title": "HTTPValidationError",
        "type": "object"
      },
      "JobCreateResponse": {
        "description": "Response payload for job creation.",
        "properties": {
          "expected_sections": {
            "description": "Total number of sections expected for the lesson job (0 for non-lesson jobs).",
            "minimum": 0.0,
            "title": "Expected Sections",
            "type": "integer"
          },
          "job_id": {
            "title": "Job Id",
            "type": "string"
          }
        },
        "required": [
          "expected_sections",
          "job_id"
        ],
        "title": "JobCreateResponse",
        "type": "object"
      },
      "JobRetryRequest": {
        "description": "Request payload for retrying a failed job.",
        "properties": {
          "agents": {
            "anyOf": [
              {
                "items": {
                  "enum": [
                    "planner",
                    "gatherer",
                    "structurer",
                    "repair",
                    "stitcher"
                  ],
                  "type": "string"
                },
                "type": "array"
              },
              {
                "type": "null"
              }
            ],
            "description": "Pipeline agents to retry (defaults to full pipeline).",
            "title": "Agents"
          },
          "sections": {
            "anyOf": [
              {
                "items": {
                  "type": "integer"
                },
                "type": "array"
              },
              {
                "type": "null"
              }
            ],
            "description": "0-based section indexes to retry (defaults to all sections).",
            "title": "Sections"
          }
        },
        "title": "JobRetryRequest",
        "type": "object"
      },
      "CurrentSectionStatus": {
        "description": "Section status metadata for streaming job progress.",
        "properties": {
          "index": {
            "description": "0-based index of the active section.",
            "minimum": 0.0,
            "title": "Index",
            "type": "integer"
          },
          "retry_count": {
            "anyOf": [
              {
                "minimum": 0.0,
                "type": "integer"
              },
              {
                "type": "null"
              }
            ],
            "description": "Retry attempts for the active section when applicable.",
            "title": "Retry Count"
          },
          "status": {
            "description": "Section generation status.",
            "title": "Status",
            "type": "string"
          },
          "title": {
            "anyOf": [
              {
                "type": "string"
              },
              {
                "type": "null"
              }
            ],
            "description": "Title of the active section when known.",
            "title": "Title"
          }
        },
        "required": [
          "index",
          "status"
        ],
        "title": "CurrentSectionStatus",
        "type": "object"
      },
      "JobStatusResponse": {
        "description": "Status payload for an asynchronous job.",
        "properties": {
          "completed_at": {
            "anyOf": [
              {
                "type": "string"
              },
              {
                "type": "null"
              }
            ],
            "title": "Completed At"
          },
          "completed_steps": {
            "anyOf": [
              {
                "minimum": 0.0,
                "type": "integer"
              },
              {
                "type": "null"
              }
            ],
            "description": "Completed progress steps when available.",
            "title": "Completed Steps"
          },
          "completed_sections": {
            "anyOf": [
              {
                "minimum": 0.0,
                "type": "integer"
              },
              {
                "type": "null"
              }
            ],
            "description": "Number of lesson sections completed so far.",
            "title": "Completed Sections"
          },
          "completed_section_indexes": {
            "anyOf": [
              {
                "items": {
                  "minimum": 0.0,
                  "type": "integer"
                },
                "type": "array"
              },
              {
                "type": "null"
              }
            ],
            "description": "0-based section indexes that have been completed so far.",
            "title": "Completed Section Indexes"
          },
          "cost": {
            "anyOf": [
              {
                "additionalProperties": true,
                "type": "object"
              },
              {
                "type": "null"
              }
            ],
            "title": "Cost"
          },
          "current_section": {
            "anyOf": [
              {
                "$ref": "#/components/schemas/CurrentSectionStatus"
              },
              {
                "type": "null"
              }
            ]
          },
          "created_at": {
            "title": "Created At",
            "type": "string"
          },
          "expected_sections": {
            "anyOf": [
              {
                "minimum": 0.0,
                "type": "integer"
              },
              {
                "type": "null"
              }
            ],
            "description": "Total number of expected sections for lesson jobs.",
            "title": "Expected Sections"
          },
          "job_id": {
            "title": "Job Id",
            "type": "string"
          },
          "logs": {
            "items": {
              "type": "string"
            },
            "title": "Logs",
            "type": "array"
          },
          "phase": {
            "anyOf": [
              {
                "type": "string"
              },
              {
                "type": "null"
              }
            ],
            "title": "Phase"
          },
          "progress": {
            "anyOf": [
              {
                "maximum": 100.0,
                "minimum": 0.0,
                "type": "number"
              },
              {
                "type": "null"
              }
            ],
            "description": "Progress percent (0-100) when available.",
            "title": "Progress"
          },
          "request": {
            "anyOf": [
              {
                "$ref": "#/components/schemas/GenerateLessonRequest"
              },
              {
                "$ref": "#/components/schemas/WritingCheckRequest"
              }
            ],
            "title": "Request"
          },
          "retry_agents": {
            "anyOf": [
              {
                "items": {
                  "type": "string"
                },
                "type": "array"
              },
              {
                "type": "null"
              }
            ],
            "description": "Pipeline agents targeted for retry, when applicable.",
            "title": "Retry Agents"
          },
          "retry_count": {
            "anyOf": [
              {
                "minimum": 0.0,
                "type": "integer"
              },
              {
                "type": "null"
              }
            ],
            "description": "Retry attempts already used for this job.",
            "title": "Retry Count"
          },
          "retry_sections": {
            "anyOf": [
              {
                "items": {
                  "minimum": 0.0,
                  "type": "integer"
                },
                "type": "array"
              },
              {
                "type": "null"
              }
            ],
            "description": "0-based section indexes targeted for retry, when applicable.",
            "title": "Retry Sections"
          },
          "result": {
            "anyOf": [
              {
                "additionalProperties": true,
                "type": "object"
              },
              {
                "type": "null"
              }
            ],
            "title": "Result"
          },
          "status": {
            "enum": [
              "queued",
              "running",
              "done",
              "error",
              "canceled"
            ],
            "title": "Status",
            "type": "string"
          },
          "subphase": {
            "anyOf": [
              {
                "type": "string"
              },
              {
                "type": "null"
              }
            ],
            "title": "Subphase"
          },
          "max_retries": {
            "anyOf": [
              {
                "minimum": 0.0,
                "type": "integer"
              },
              {
                "type": "null"
              }
            ],
            "description": "Maximum retry attempts allowed for this job.",
            "title": "Max Retries"
          },
          "total_steps": {
            "anyOf": [
              {
                "minimum": 1.0,
                "type": "integer"
              },
              {
                "type": "null"
              }
            ],
            "description": "Total number of progress steps when available.",
            "title": "Total Steps"
          },
          "updated_at": {
            "title": "Updated At",
            "type": "string"
          },
          "validation": {
            "anyOf": [
              {
                "$ref": "#/components/schemas/ValidationResponse"
              },
              {
                "type": "null"
              }
            ]
          }
        },
        "required": [
          "job_id",
          "status",
          "request",
          "created_at",
          "updated_at"
        ],
        "title": "JobStatusResponse",
        "type": "object"
      },
      "LessonMeta": {
        "description": "Metadata about the lesson generation process.",
        "properties": {
          "latency_ms": {
            "title": "Latency Ms",
            "type": "integer"
          },
          "model_a": {
            "title": "Model A",
            "type": "string"
          },
          "model_b": {
            "title": "Model B",
            "type": "string"
          },
          "provider_a": {
            "title": "Provider A",
            "type": "string"
          },
          "provider_b": {
            "title": "Provider B",
            "type": "string"
          }
        },
        "required": [
          "provider_a",
          "model_a",
          "provider_b",
          "model_b",
          "latency_ms"
        ],
        "title": "LessonMeta",
        "type": "object"
      },
      "LessonRecordResponse": {
        "description": "Response payload for lesson retrieval.",
        "properties": {
          "created_at": {
            "title": "Created At",
            "type": "string"
          },
          "lesson_id": {
            "title": "Lesson Id",
            "type": "string"
          },
          "lesson_json": {
            "additionalProperties": true,
            "title": "Lesson Json",
            "type": "object"
          },
          "meta": {
            "$ref": "#/components/schemas/LessonMeta"
          },
          "prompt_version": {
            "title": "Prompt Version",
            "type": "string"
          },
          "schema_version": {
            "title": "Schema Version",
            "type": "string"
          },
          "title": {
            "title": "Title",
            "type": "string"
          },
          "topic": {
            "title": "Topic",
            "type": "string"
          }
        },
        "required": [
          "lesson_id",
          "topic",
          "title",
          "created_at",
          "schema_version",
          "prompt_version",
          "lesson_json",
          "meta"
        ],
        "title": "LessonRecordResponse",
        "type": "object"
      },
      "ModelsConfig": {
        "additionalProperties": false,
        "description": "Per-agent model selection overrides.",
        "properties": {
          "gatherer_model": {
            "anyOf": [
              {
                "type": "string"
              },
              {
                "type": "null"
              }
            ],
            "description": "Model used for the gatherer agent (provider inferred when possible).",
            "examples": [
              "xiaomi/mimo-v2-flash:free"
            ],
            "title": "Gatherer Model"
          },
          "planner_model": {
            "anyOf": [
              {
                "type": "string"
              },
              {
                "type": "null"
              }
            ],
            "description": "Model used for the planner agent (provider inferred when possible).",
            "examples": [
              "openai/gpt-oss-120b:free"
            ],
            "title": "Planner Model"
          },
          "repairer_model": {
            "anyOf": [
              {
                "type": "string"
              },
              {
                "type": "null"
              }
            ],
            "description": "Model used for repair validation and fixes (provider inferred when possible).",
            "examples": [
              "google/gemma-3-27b-it:free"
            ],
            "title": "Repairer Model"
          },
          "structurer_model": {
            "anyOf": [
              {
                "type": "string"
              },
              {
                "type": "null"
              }
            ],
            "description": "Model used for lesson structuring (provider inferred when possible).",
            "examples": [
              "openai/gpt-oss-20b:free"
            ],
            "title": "Structurer Model"
          }
        },
        "title": "ModelsConfig",
        "type": "object"
      },
      "OrchestrationFailureResponse": {
        "description": "Response payload for orchestration failures.",
        "properties": {
          "detail": {
            "title": "Detail",
            "type": "string"
          },
          "error": {
            "title": "Error",
            "type": "string"
          },
          "logs": {
            "items": {
              "type": "string"
            },
            "title": "Logs",
            "type": "array"
          }
        },
        "required": [
          "detail",
          "error",
          "logs"
        ],
        "title": "OrchestrationFailureResponse",
        "type": "object"
      },
      "ValidationError": {
        "properties": {
          "loc": {
            "items": {
              "anyOf": [
                {
                  "type": "string"
                },
                {
                  "type": "integer"
                }
              ]
            },
            "title": "Location",
            "type": "array"
          },
          "msg": {
            "title": "Message",
            "type": "string"
          },
          "type": {
            "title": "Error Type",
            "type": "string"
          }
        },
        "required": [
          "loc",
          "msg",
          "type"
        ],
        "title": "ValidationError",
        "type": "object"
      },
      "ValidationResponse": {
        "description": "Response model for lesson validation results.",
        "properties": {
          "errors": {
            "items": {
              "type": "string"
            },
            "title": "Errors",
            "type": "array"
          },
          "ok": {
            "title": "Ok",
            "type": "boolean"
          }
        },
        "required": [
          "ok",
          "errors"
        ],
        "title": "ValidationResponse",
        "type": "object"
      },
      "WritingCheckRequest": {
        "additionalProperties": false,
        "description": "Request payload for response evaluation.",
        "properties": {
          "checker_model": {
            "anyOf": [
              {
                "type": "string"
              },
              {
                "type": "null"
              }
            ],
            "description": "Optional model override for writing evaluation (provider inferred when possible).",
            "examples": [
              "openai/gpt-oss-20b:free"
            ],
            "title": "Checker Model"
          },
          "criteria": {
            "additionalProperties": true,
            "description": "The evaluation criteria from the lesson.",
            "title": "Criteria",
            "type": "object"
          },
          "text": {
            "description": "The user-written response to check (max 300 words).",
            "minLength": 1,
            "title": "Text",
            "type": "string"
          }
        },
        "required": [
          "text",
          "criteria"
        ],
        "title": "WritingCheckRequest",
        "type": "object"
      },
      "LessonCatalogResponse": {
        "description": "Response payload for lesson option metadata.",
        "properties": {
          "blueprints": {
            "items": {
              "$ref": "#/components/schemas/OptionDetail"
            },
            "title": "Blueprints",
            "type": "array"
          },
          "teaching_styles": {
            "items": {
              "$ref": "#/components/schemas/OptionDetail"
            },
            "title": "Teaching Styles",
            "type": "array"
          },
          "learner_levels": {
            "items": {
              "$ref": "#/components/schemas/OptionDetail"
            },
            "title": "Learner Levels",
            "type": "array"
          },
          "depths": {
            "items": {
              "$ref": "#/components/schemas/OptionDetail"
            },
            "title": "Depths",
            "type": "array"
          },
          "widgets": {
            "items": {
              "$ref": "#/components/schemas/OptionDetail"
            },
            "title": "Widgets",
            "type": "array"
          },
          "agent_models": {
            "items": {
              "$ref": "#/components/schemas/AgentModelOption"
            },
            "title": "Agent Models",
            "type": "array"
          },
          "default_widgets": {
            "additionalProperties": {
              "additionalProperties": {
                "items": {
                  "type": "string"
                },
                "type": "array"
              },
              "type": "object"
            },
            "title": "Default Widgets",
            "type": "object"
          }
        },
        "required": [
          "blueprints",
          "teaching_styles",
          "learner_levels",
          "depths",
          "widgets",
          "agent_models",
          "default_widgets"
        ],
        "title": "LessonCatalogResponse",
        "type": "object"
      },
      "OptionDetail": {
        "description": "Metadata describing an option with tooltip guidance.",
        "properties": {
          "id": {
            "title": "Id",
            "type": "string"
          },
          "label": {
            "title": "Label",
            "type": "string"
          },
          "tooltip": {
            "title": "Tooltip",
            "type": "string"
          }
        },
        "required": [
          "id",
          "label",
          "tooltip"
        ],
        "title": "OptionDetail",
        "type": "object"
      },
      "AgentModelOption": {
        "description": "Valid model choices for a pipeline agent.",
        "properties": {
          "agent": {
            "title": "Agent",
            "type": "string"
          },
          "default": {
            "anyOf": [
              {
                "type": "string"
              },
              {
                "type": "null"
              }
            ],
            "title": "Default"
          },
          "options": {
            "items": {
              "type": "string"
            },
            "title": "Options",
            "type": "array"
          }
        },
        "required": [
          "agent",
          "default",
          "options"
        ],
        "title": "AgentModelOption",
        "type": "object"
      }
    }
  },
  "info": {
    "title": "FastAPI",
    "version": "0.1.0"
  },
  "openapi": "3.1.0",
  "paths": {
    "/health": {
      "get": {
        "operationId": "health_health_get",
        "responses": {
          "200": {
            "content": {
              "application/json": {
                "schema": {
                  "additionalProperties": {
                    "type": "string"
                  },
                  "title": "Response Health Health Get",
                  "type": "object"
                }
              }
            },
            "description": "Successful Response"
          }
        },
        "summary": "Health"
      }
    },
    "/v1/jobs": {
      "post": {
        "description": "Create a background lesson generation job.",
        "operationId": "create_job_v1_jobs_post",
        "parameters": [
          {
            "in": "header",
            "name": "X-DGS-Dev-Key",
            "required": true,
            "schema": {
              "title": "X-Dgs-Dev-Key",
              "type": "string"
            }
          }
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/GenerateLessonRequest"
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/JobCreateResponse"
                }
              }
            },
            "description": "Successful Response"
          },
          "422": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/HTTPValidationError"
                }
