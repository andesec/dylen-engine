{
  "components": {
    "schemas": {
      "GenerateLessonRequest": {
        "additionalProperties": false,
        "description": "Request payload for lesson generation.",
        "properties": {
          "constraints": {
            "anyOf": [
              {
                "$ref": "#/components/schemas/LessonConstraints"
              },
              {
                "type": "null"
              }
            ]
          },
          "idempotency_key": {
            "anyOf": [
              {
                "type": "string"
              },
              {
                "type": "null"
              }
            ],
            "title": "Idempotency Key"
          },
          "mode": {
            "anyOf": [
              {
                "enum": [
                  "fast",
                  "balanced",
                  "best"
                ],
                "type": "string"
              },
              {
                "type": "null"
              }
            ],
            "default": "balanced",
            "title": "Mode"
          },
          "schema_version": {
            "anyOf": [
              {
                "type": "string"
              },
              {
                "type": "null"
              }
            ],
            "title": "Schema Version"
          },
          "topic": {
            "minLength": 1,
            "title": "Topic",
            "type": "string"
          },
          "topic_details": {
            "anyOf": [
              {
                "type": "string"
              },
              {
                "type": "null"
              }
            ],
            "title": "Topic Details"
          }
        },
        "required": [
          "topic"
        ],
        "title": "GenerateLessonRequest",
        "type": "object"
      },
      "GenerateLessonResponse": {
        "description": "Response payload for lesson generation.",
        "properties": {
          "lesson_id": {
            "title": "Lesson Id",
            "type": "string"
          },
          "lesson_json": {
            "additionalProperties": true,
            "title": "Lesson Json",
            "type": "object"
          },
          "logs": {
            "items": {
              "type": "string"
            },
            "title": "Logs",
            "type": "array"
          },
          "meta": {
            "$ref": "#/components/schemas/LessonMeta"
          }
        },
        "required": [
          "lesson_id",
          "lesson_json",
          "meta",
          "logs"
        ],
        "title": "GenerateLessonResponse",
        "type": "object"
      },
      "HTTPValidationError": {
        "properties": {
          "detail": {
            "items": {
              "$ref": "#/components/schemas/ValidationError"
            },
            "title": "Detail",
            "type": "array"
          }
        },
        "title": "HTTPValidationError",
        "type": "object"
      },
      "LessonConstraints": {
        "additionalProperties": false,
        "description": "Optional constraints for lesson generation.",
        "properties": {
          "language": {
            "anyOf": [
              {
                "type": "string"
              },
              {
                "type": "null"
              }
            ],
            "title": "Language"
          },
          "learnerLevel": {
            "anyOf": [
              {
                "enum": [
                  "Newbie",
                  "Beginner",
                  "Intermediate",
                  "Expert"
                ],
                "type": "string"
              },
              {
                "type": "null"
              }
            ],
            "title": "Learnerlevel"
          },
          "length": {
            "anyOf": [
              {
                "enum": [
                  "Highlights",
                  "Detailed",
                  "Training"
                ],
                "type": "string"
              },
              {
                "type": "null"
              }
            ],
            "title": "Length"
          }
        },
        "title": "LessonConstraints",
        "type": "object"
      },
      "LessonMeta": {
        "description": "Metadata about the lesson generation process.",
        "properties": {
          "latency_ms": {
            "title": "Latency Ms",
            "type": "integer"
          },
          "model_a": {
            "title": "Model A",
            "type": "string"
          },
          "model_b": {
            "title": "Model B",
            "type": "string"
          },
          "provider_a": {
            "title": "Provider A",
            "type": "string"
          },
          "provider_b": {
            "title": "Provider B",
            "type": "string"
          }
        },
        "required": [
          "provider_a",
          "model_a",
          "provider_b",
          "model_b",
          "latency_ms"
        ],
        "title": "LessonMeta",
        "type": "object"
      },
      "LessonRecordResponse": {
        "description": "Response payload for lesson retrieval.",
        "properties": {
          "created_at": {
            "title": "Created At",
            "type": "string"
          },
          "lesson_id": {
            "title": "Lesson Id",
            "type": "string"
          },
          "lesson_json": {
            "additionalProperties": true,
            "title": "Lesson Json",
            "type": "object"
          },
          "meta": {
            "$ref": "#/components/schemas/LessonMeta"
          },
          "prompt_version": {
            "title": "Prompt Version",
            "type": "string"
          },
          "schema_version": {
            "title": "Schema Version",
            "type": "string"
          },
          "title": {
            "title": "Title",
            "type": "string"
          },
          "topic": {
            "title": "Topic",
            "type": "string"
          }
        },
        "required": [
          "lesson_id",
          "topic",
          "title",
          "created_at",
          "schema_version",
          "prompt_version",
          "lesson_json",
          "meta"
        ],
        "title": "LessonRecordResponse",
        "type": "object"
      },
      "ValidationError": {
        "properties": {
          "loc": {
            "items": {
              "anyOf": [
                {
                  "type": "string"
                },
                {
                  "type": "integer"
                }
              ]
            },
            "title": "Location",
            "type": "array"
          },
          "msg": {
            "title": "Message",
            "type": "string"
          },
          "type": {
            "title": "Error Type",
            "type": "string"
          }
        },
        "required": [
          "loc",
          "msg",
          "type"
        ],
        "title": "ValidationError",
        "type": "object"
      },
      "ValidationResponse": {
        "description": "Response model for lesson validation results.",
        "properties": {
          "errors": {
            "items": {
              "type": "string"
            },
            "title": "Errors",
            "type": "array"
          },
          "ok": {
            "title": "Ok",
            "type": "boolean"
          }
        },
        "required": [
          "ok",
          "errors"
        ],
        "title": "ValidationResponse",
        "type": "object"
      }
    }
  },
  "info": {
    "title": "FastAPI",
    "version": "0.1.0"
  },
  "openapi": "3.1.0",
  "paths": {
    "/health": {
      "get": {
        "operationId": "health_health_get",
        "responses": {
          "200": {
            "content": {
              "application/json": {
                "schema": {
                  "additionalProperties": {
                    "type": "string"
                  },
                  "title": "Response Health Health Get",
                  "type": "object"
                }
              }
            },
            "description": "Successful Response"
          }
        },
        "summary": "Health"
      }
    },
    "/v1/lessons/generate": {
      "post": {
        "description": "Generate a lesson from a topic using the two-step pipeline.",
        "operationId": "generate_lesson_v1_lessons_generate_post",
        "parameters": [
          {
            "in": "header",
            "name": "X-DGS-Dev-Key",
            "required": true,
            "schema": {
              "title": "X-Dgs-Dev-Key",
              "type": "string"
            }
          }
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/GenerateLessonRequest"
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/GenerateLessonResponse"
                }
              }
            },
            "description": "Successful Response"
          },
          "422": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/HTTPValidationError"
                }
              }
            },
            "description": "Validation Error"
          }
        },
        "summary": "Generate Lesson"
      }
    },
    "/v1/lessons/validate": {
      "post": {
        "description": "Validate a lesson payload against schema + widget registry.",
        "operationId": "validate_endpoint_v1_lessons_validate_post",
        "parameters": [
          {
            "in": "header",
            "name": "X-DGS-Dev-Key",
            "required": true,
            "schema": {
              "title": "X-Dgs-Dev-Key",
              "type": "string"
            }
          }
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "additionalProperties": true,
                "title": "Payload",
                "type": "object"
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ValidationResponse"
                }
              }
            },
            "description": "Successful Response"
          },
          "422": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/HTTPValidationError"
                }
              }
            },
            "description": "Validation Error"
          }
        },
        "summary": "Validate Endpoint"
      }
    },
    "/v1/lessons/{lesson_id}": {
      "get": {
        "description": "Fetch a stored lesson by identifier.",
        "operationId": "get_lesson_v1_lessons__lesson_id__get",
        "parameters": [
          {
            "in": "path",
            "name": "lesson_id",
            "required": true,
            "schema": {
              "title": "Lesson Id",
              "type": "string"
            }
          },
          {
            "in": "header",
            "name": "X-DGS-Dev-Key",
            "required": true,
            "schema": {
              "title": "X-Dgs-Dev-Key",
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/LessonRecordResponse"
                }
              }
            },
            "description": "Successful Response"
          },
          "422": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/HTTPValidationError"
                }
              }
            },
            "description": "Validation Error"
          }
        },
        "summary": "Get Lesson"
      }
    }
  }
}
