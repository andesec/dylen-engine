  "components": {
    "schemas": {
      "GenerateLessonRequest-Input": {
        "properties": {
          "topic": {
            "type": "string",
            "minLength": 1,
            "title": "Topic",
            "description": "Topic to generate a lesson for.",
            "examples": [
              "Introduction to Python"
            ]
          },
          "prompt": {
            "anyOf": [
              {
                "type": "string",
                "minLength": 1
              },
              {
                "type": "null"
              }
            ],
            "title": "Prompt",
            "description": "Optional user-supplied guidance (max 250 words).",
            "examples": [
              "Focus on lists and loops"
            ]
          },
          "mode": {
            "$ref": "#/components/schemas/GenerationMode",
            "description": "Generation quality mode (fast, balanced, best).",
            "default": "balanced"
          },
          "schema_version": {
            "anyOf": [
              {
                "type": "string"
              },
              {
                "type": "null"
              }
            ],
            "title": "Schema Version",
            "description": "Optional schema version to pin the lesson output to."
          },
          "idempotency_key": {
            "anyOf": [
              {
                "type": "string"
              },
              {
                "type": "null"
              }
            ],
            "title": "Idempotency Key",
            "description": "Idempotency key to prevent duplicate lesson generation."
          },
          "constraints": {
            "anyOf": [
              {
                "$ref": "#/components/schemas/GenerationConstraints"
              },
              {
                "type": "null"
              }
            ],
            "description": "Domain-specific generation constraints."
          },
          "models": {
            "anyOf": [
              {
                "$ref": "#/components/schemas/ModelsConfig"
              },
              {
                "type": "null"
              }
            ],
            "description": "Optional per-agent model selection overrides."
          }
        },
        "additionalProperties": false,
        "type": "object",
        "required": [
          "topic"
        ],
        "title": "GenerateLessonRequest",
        "description": "Request payload for lesson generation."
      },
      "GenerateLessonRequest-Output": {
        "properties": {
          "topic": {
            "type": "string",
            "minLength": 1,
            "title": "Topic",
            "description": "Topic to generate a lesson for.",
            "examples": [
              "Introduction to Python"
            ]
          },
          "prompt": {
            "anyOf": [
              {
                "type": "string",
                "minLength": 1
              },
              {
                "type": "null"
              }
            ],
            "title": "Prompt",
            "description": "Optional user-supplied guidance (max 250 words).",
            "examples": [
              "Focus on lists and loops"
            ]
          },
          "mode": {
            "$ref": "#/components/schemas/GenerationMode",
            "description": "Generation quality mode (fast, balanced, best).",
            "default": "balanced"
          },
          "schema_version": {
            "anyOf": [
              {
                "type": "string"
              },
              {
                "type": "null"
              }
            ],
            "title": "Schema Version",
            "description": "Optional schema version to pin the lesson output to."
          },
          "idempotency_key": {
            "anyOf": [
              {
                "type": "string"
              },
              {
                "type": "null"
              }
            ],
            "title": "Idempotency Key",
            "description": "Idempotency key to prevent duplicate lesson generation."
          },
          "constraints": {
            "anyOf": [
              {
                "$ref": "#/components/schemas/GenerationConstraints"
              },
              {
                "type": "null"
              }
            ],
            "description": "Domain-specific generation constraints."
          },
          "models": {
            "anyOf": [
              {
                "$ref": "#/components/schemas/ModelsConfig"
              },
              {
                "type": "null"
              }
            ],
            "description": "Optional per-agent model selection overrides."
          }
        },
        "additionalProperties": false,
        "type": "object",
        "required": [
          "topic"
        ],
        "title": "GenerateLessonRequest",
        "description": "Request payload for lesson generation."
      },
      "GenerateLessonResponse": {
        "properties": {
          "lesson_id": {
            "type": "string",
            "title": "Lesson Id"
          },
          "lesson_json": {
            "additionalProperties": true,
            "type": "object",
            "title": "Lesson Json"
          },
          "meta": {
            "$ref": "#/components/schemas/LessonMeta"
          },
          "logs": {
            "items": {
              "type": "string"
            },
            "type": "array",
            "title": "Logs"
          }
        },
        "type": "object",
        "required": [
          "lesson_id",
          "lesson_json",
          "meta",
          "logs"
        ],
        "title": "GenerateLessonResponse",
        "description": "Response payload for lesson generation."
      },
      "GenerationConstraints": {
        "properties": {
          "primaryLanguage": {
            "anyOf": [
              {
                "type": "string",
                "enum": [
                  "English",
                  "German",
                  "Urdu"
                ]
              },
              {
                "type": "null"
              }
            ],
            "title": "Primarylanguage",
            "default": "English"
          },
          "learnerLevel": {
            "anyOf": [
              {
                "type": "string",
                "enum": [
                  "Newbie",
                  "Beginner",
                  "Intermediate",
                  "Expert"
                ]
              },
              {
                "type": "null"
              }
            ],
            "title": "Learnerlevel"
          },
          "depth": {
            "anyOf": [
              {
                "type": "string",
                "enum": [
                  "2",
                  "3",
                  "4",
                  "5",
                  "6",
                  "7",
                  "8",
                  "9",
                  "10"
                ]
              },
              {
                "type": "null"
              }
            ],
            "title": "Depth",
            "default": "2"
          }
        },
        "type": "object",
        "title": "GenerationConstraints",
        "description": "Specific constraints for lesson content generation."
      },
      "GenerationMode": {
        "type": "string",
        "enum": [
          "fast",
          "balanced",
          "best"
        ],
        "title": "GenerationMode",
        "description": "Quality/speed tradeoff for lesson generation."
      },
      "HTTPValidationError": {
        "properties": {
          "detail": {
            "items": {
              "$ref": "#/components/schemas/ValidationError"
            },
            "type": "array",
            "title": "Detail"
          }
        },
        "type": "object",
        "title": "HTTPValidationError"
      },
      "JobCreateResponse": {
        "properties": {
          "jobId": {
            "type": "string",
            "title": "Jobid"
          }
        },
        "type": "object",
        "required": [
          "jobId"
        ],
        "title": "JobCreateResponse",
        "description": "Response payload for job creation."
      },
      "JobStatusResponse": {
        "properties": {
          "jobId": {
            "type": "string",
            "title": "Jobid"
          },
          "status": {
            "type": "string",
            "enum": [
              "queued",
              "running",
              "done",
              "error",
              "canceled"
            ],
            "title": "Status"
          },
          "phase": {
            "anyOf": [
              {
                "type": "string"
              },
              {
                "type": "null"
              }
            ],
            "title": "Phase"
          },
          "subphase": {
            "anyOf": [
              {
                "type": "string"
              },
              {
                "type": "null"
              }
            ],
            "title": "Subphase"
          },
          "total_steps": {
            "anyOf": [
              {
                "type": "integer",
                "minimum": 1.0
              },
              {
                "type": "null"
              }
            ],
            "title": "Total Steps",
            "description": "Total number of progress steps when available."
          },
          "completed_steps": {
            "anyOf": [
              {
                "type": "integer",
                "minimum": 0.0
              },
              {
                "type": "null"
              }
            ],
            "title": "Completed Steps",
            "description": "Completed progress steps when available."
          },
          "progress": {
            "anyOf": [
              {
                "type": "number",
                "maximum": 100.0,
                "minimum": 0.0
              },
              {
                "type": "null"
              }
            ],
            "title": "Progress",
            "description": "Progress percent (0-100) when available."
          },
          "logs": {
            "items": {
              "type": "string"
            },
            "type": "array",
            "title": "Logs"
          },
          "request": {
            "$ref": "#/components/schemas/GenerateLessonRequest-Output"
          },
          "result": {
            "anyOf": [
              {
                "additionalProperties": true,
                "type": "object"
              },
              {
                "type": "null"
              }
            ],
            "title": "Result"
          },
          "validation": {
            "anyOf": [
              {
                "$ref": "#/components/schemas/ValidationResponse"
              },
              {
                "type": "null"
              }
            ]
          },
          "cost": {
            "anyOf": [
              {
                "additionalProperties": true,
                "type": "object"
              },
              {
                "type": "null"
              }
            ],
            "title": "Cost"
          },
          "createdAt": {
            "type": "string",
            "title": "Createdat"
          },
          "updatedAt": {
            "type": "string",
            "title": "Updatedat"
          },
          "completedAt": {
            "anyOf": [
              {
                "type": "string"
              },
              {
                "type": "null"
              }
            ],
            "title": "Completedat"
          }
        },
        "type": "object",
        "required": [
          "jobId",
          "status",
          "request",
          "createdAt",
          "updatedAt"
        ],
        "title": "JobStatusResponse",
        "description": "Status payload for an asynchronous lesson generation job."
      },
      "KnowledgeModel": {
        "type": "string",
        "enum": [
          "gemini-2.5-flash",
          "gemini-2.5-pro",
          "xiaomi/mimo-v2-flash:free",
          "deepseek/deepseek-r1-0528:free",
          "openai/gpt-oss-120b:free"
        ],
        "title": "KnowledgeModel",
        "description": "Model options for the knowledge collection agent."
      },
      "LessonMeta": {
        "properties": {
          "provider_a": {
